{
    "name": "nh/starter-pack",
    "description": "Starter pack presets (Frontend/Backend)",
    "license": "MIT",
    "type": "library",
    "require": {
      "nh/bs-component": "4.0.*",
      "nh/access-control": "1.5.*",
      "nh/mediable": "1.6.*",
      "nh/sortable": "1.3.*",
      "nh/searchable": "1.1.*",
      "nh/trackable": "1.7.*"
    },
    "author": {
        "name": "Natacha Herth",
        "email": "info@natachaherth.ch"
    },
    "autoload": {
        "files": [
            "src/Helpers/NotificationHelper.php",
            "src/Helpers/RequestCleanHelper.php",
            "src/Helpers/SyncHelper.php"
        ],
        "psr-4": {
            "Nh\\StarterPack\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nh\\StarterPack\\StarterPackServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
